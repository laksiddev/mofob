                name="myItineraryLookupServiceBinding" />
      <endpoint address="net.tcp://demobiztalk.demo.local/MOF.ItineraryLookupService/ItineraryLookupService.svc"
                binding="netTcpBinding" bindingConfiguration=""
                contract="MOF.BizTalkESB.IItineraryLookupService"
                name="myItineraryLookupServiceTcpBinding" />
    </client>
  </system.serviceModel>
  <applicationSettings>
